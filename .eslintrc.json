{
  "env": {
    "node": true,
    "commonjs": true,
    "es2020": true
  },
  "extends": [
    "standard",
    "prettier",
    "prettier/standard",
    "plugin:node/recommended",
    "eslint:recommended"
  ],
  "plugins": ["prettier", "babel", "standard"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 11
  },
  "rules": {
    "prettier/prettier": "error",
    "func-names": "off"
  }
}
