{
  "name": "@mattleite/eslint-prettier-standardjs",
  "version": "1.0.0",
  "description": "ESLint Package with Prettier & Standard with Babel Base install",
  "main": "index.js",
  "scripts": {
    "test": "eslint",
    "lint": "eslint \"**/*.js\"",
    "build": "babel src --out-dir dist --ignore node_modules",
    "p": "prettier --write 'src/**/*.js'"
